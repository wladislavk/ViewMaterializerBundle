{
    "name": "vkr/view-materializer-bundle",
    "type": "symfony-bundle",
    "description": "A bundle for emulating materialized views in MySQL, made for Symfony2/3 and Doctrine",
    "keywords": ["mysql", "views", "doctrine", "symfony2", "symfony3"],
    "homepage": "https://github.com/wladislavk/ViewMaterializerBundle",
    "license": "MIT",
    "authors": [
        {"name": "Vladislav Kryshtanovskiy", "email": "wladislav.k@gmail.com"}
    ],
    "require": {
        "php": ">=5.6",
        "symfony/symfony": "~2.8|~3.0",
        "doctrine/orm": ">=2.2.3",
        "doctrine/doctrine-bundle": "~1.4",
        "vkr/custom-logger-bundle": "~1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7"
    },
    "autoload": {
        "psr-4": {
            "VKR\\ViewMaterializerBundle\\": ""
        }
    }
}
